<head>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
        {{! <link href="/css/about.css" rel="stylesheet"> }}
<link href="/css/dashboard.css" rel="stylesheet">
</head>
{{> header}}
<body>
    <main class="main-content">
    {{> admin_views/admin_sidebar}}
            <div class="content">
            <div class="d-flex justify-content-between align-items-center mb-3">
    <a href="/add_session" class="btn btn-primary">Add Session</a>
    <a href="/edit_sessions" class="btn btn-secondary">Edit Sessions</a>
    <a href="/delete_sessions" class="btn btn-danger">Delete Sessions</a>
    </div>

    <table class="table">
        <thead>
            <tr>
                <th>Session Name</th>
                <th>Date</th>
                <th>Time</th>
                <th>Venue</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <!-- {{#sessions}} -->
            <tr>
                <td>{{sessionName}}</td>
                <td>{{date}}</td>
                <td>{{startTime}} - {{endTime}}</td>
                <td>{{venue}}</td>
                <td>
                    <button class="btn btn-info">Edit</button>
                </td>
            </tr>
            <!-- {{/sessions}} -->

            <!-- {{^sessions}} -->
            <tr>
                <td colspan="5">No sessions in the database.</td>
            </tr>
            <!-- {{/sessions}} -->
        </tbody>
    </table>

        </div>
        <!-- Your dynamic content goes here -->
    </main>
</body>
{{> footer}}
